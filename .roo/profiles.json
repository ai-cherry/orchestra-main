  "modelRouting": {
    "ui-design": {
      "priority": ["gpt41-ui-premium", "claude-opus4", "gemini-25-pro"],
      "costLimit": 0.25,
      "tokenLimit": 4096
    },
    "architecture": {
      "priority": ["claude-opus4", "claude-sonnet4", "gpt41-ui-premium"],
      "costLimit": 0.30,
      "tokenLimit": 4096
    },
    "implementation": {
      "priority": ["claude-sonnet4", "gpt41-mini", "gemini-25-pro"],
      "costLimit": 0.15,
      "tokenLimit": 4096
    },
    "research": {
      "priority": ["gemini-25-pro", "claude-opus4", "gpt41-ui-premium"],
      "costLimit": 0.20,
      "tokenLimit": 4096
    }
  },
  "rateLimits": {
    "gpt41-ui-premium": {
      "rpm": 150,
      "tpm": 60000
    },
    "claude-opus4": {
      "rpm": 100,
      "tpm": 40000
    },
    "claude-sonnet4": {
      "rpm": 200,
      "tpm": 50000
    },
    "gemini-25-pro": {
      "rpm": 180,
      "tpm": 45000
    }
  },
  "costOptimization": {
    "enableCascading": true,
    "budgetThresholds": {
      "low": 0.10,
      "medium": 0.20,
      "high": 0.30
    },
    "fallbackChain": {
      "premium": ["claude-opus4", "gpt41-ui-premium", "gemini-25-pro"],
      "balanced": ["claude-sonnet4", "gpt41-mini"],
      "economy": ["gpt41-mini"]
    }
  }
}