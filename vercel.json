{
  "version": 2,
  "builds": [
    {
      "src": "modern-admin/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/admin/(.*)",
      "dest": "/modern-admin/dist/$1"
    },
    {
      "src": "/admin",
      "dest": "/modern-admin/dist/index.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

