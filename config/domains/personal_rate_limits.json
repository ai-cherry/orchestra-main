{
  "domain": "personal",
  "rate_limiting": {
    "enabled": true,
    "rules": {
      "global": {
        "requests_per_minute": 100,
        "burst_size": 150,
        "cooldown_seconds": 60
      },
      "endpoints": {
        "/personal/api/*": {
          "requests_per_minute": 60,
          "burst_size": 90
        },
        "/personal/webhooks/*": {
          "requests_per_minute": 30,
          "burst_size": 45
        },
        "/personal/health": {
          "requests_per_minute": 300,
          "burst_size": 300
        }
      },
      "user_tiers": {
        "free": {
          "requests_per_minute": 20,
          "burst_size": 30
        },
        "premium": {
          "requests_per_minute": 100,
          "burst_size": 150
        },
        "enterprise": {
          "requests_per_minute": 500,
          "burst_size": 750
        }
      }
    }
  },
  "circuit_breaker": {
    "enabled": true,
    "failure_threshold": 5,
    "recovery_timeout": 60,
    "success_threshold": 2
  },
  "retry_policy": {
    "max_retries": 3,
    "base_delay_ms": 100,
    "max_delay_ms": 5000,
    "exponential_base": 2
  }
}