{
  "health_check_interval": 60,
  "metrics_collection": {
    "enabled": true,
    "interval": 30,
    "metrics": [
      "workflow_execution_time",
      "agent_response_time",
      "mcp_server_uptime",
      "error_rate",
      "task_success_rate"
    ]
  },
  "alerts": {
    "email": "",
    "webhook": "",
    "conditions": [
      {
        "metric": "error_rate",
        "threshold": 0.1,
        "action": "alert"
      },
      {
        "metric": "mcp_server_uptime",
        "threshold": 0.95,
        "action": "restart"
      }
    ]
  }
}