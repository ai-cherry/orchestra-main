
[[vercel_deployment]]
auto_fix = true
monitor_deployments = true
auto_retry_count = 3
notify_on_failure = true

[[vercel_deployment.patterns]]
error = "Command 'vite build' exited with 127"
fix = "cursor_ai_enhanced.py fix_vercel_build_command"

[[vercel_deployment.patterns]]
error = "Invalid request: "files" field should be an array"
fix = "cursor_ai_enhanced.py fix_vercel_deployment_payload"

[[vercel_deployment.patterns]]
error = "Not Found"
fix = "cursor_ai_enhanced.py fix_vercel_github_integration"
